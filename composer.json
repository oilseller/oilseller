{
    "name": "oilseller/oilseller",
    "license": "MIT",
    "authors": [
        {
            "name": "rrylee",
            "email": "rrylee1994@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OilSeller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "OilSeller\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "livewire/livewire": "^1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0",
        "laravel/framework": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0",
        "doctrine/dbal": "^2.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OilSeller\\OilSellerServiceProvider"
            ],
            "aliases": {
                "OilSeller": "OilSeller\\OilSeller"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
